
<%= form_for :process_information, method: :patch, :html => {:class => "form-horizontal", :role => "form"}, url:process_information_path do |f|%>

    <hr/>
    <div class="form-group">
        <label class="col-md-12">Show User Group</label>
        <div class="col-md-7">
            <!--
            <%=f.select :category_id, @categories, {}, { class: "form-control" } %> 
            -->
            <select class="form-control">
                <option>Department 1</option>
                <option>Department 2</option>
                <option>Department 3</option>
                <option>Department 4</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-12">Execution User Group</label>
        <div class="col-md-7">
            <!--
            <%=f.select :category_id, @categories, {}, { class: "form-control" } %> 
            -->
            <select class="form-control">
                <option>Department 1</option>
                <option>Department 2</option>
                <option>Department 3</option>
                <option>Department 4</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-12">Edit User Group</label>
        <div class="col-md-7">
            <!--
            <%=f.select :category_id, @categories, {}, { class: "form-control" } %> 
            -->
            <select class="form-control">
                <option>Department 1</option>
                <option>Department 2</option>
                <option>Department 3</option>
                <option>Department 4</option>
            </select>
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-12">Delete User Group</label>
        <div class="col-md-7">
            <!--
            <%=f.select :category_id, @categories, {}, { class: "form-control" } %> 
            -->
            <select class="form-control">
                <option>Department 1</option>
                <option>Department 2</option>
                <option>Department 3</option>
                <option>Department 4</option>
            </select>
        </div>
    </div>
    <hr/>
    <div class="form-group">
        <!-- Button trigger modal -->
        <div class="col-md-2">
            <%= f.submit :"Last", :class => "btn btn-primary btn-block" %>
        </div>
        <div class="col-md-offset-6 col-md-2">
            <%= f.submit :"Save", :class => "btn btn-primary btn-block" %>
        </div>
        <div class="col-md-2">
            <%= f.submit :"Next", :class => "btn btn-primary btn-block" %>
        </div>
    </div>

<% end %>
