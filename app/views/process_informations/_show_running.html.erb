<form class="form-horizontal" role="form">
    <!-- 抄的运行 -->
    <div class="form-group">
        <div class="col-md-12"><h4>Input data</h4></div>
    </div>

    <div class="form-group">
        <label class="col-md-12">Input data from local file</label>
        <div class="col-md-4">
            <input type="file" id="caseInputFile">
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-2">
            <button class="btn btn-primary btn-block">Submit</button>
        </div>
    </div>


    <br/>

    <div class="form-group">
        <div class="col-md-12">
            <table class="table table-bordered">
                <tr>
                    <th>id</th>
                    <th>Tag</th>
                    <th>Attribute1</th>
                    <th>Attribute2</th>
                </tr>
                <tr>
                    <td>1</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td>2</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td>3</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
            </table>
            <ul class="pagination">
                <li>
                <a href="#" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                </a>
                </li>
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li>
                <a href="#" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
                </li>
            </ul>
        </div>
    </div>
    <hr/>

    <div class="form-group">
        <div class="col-md-8">
            <h4>Adjust parameters</h4>
        </div>
    </div>
    <div class="form-group">
            <label class="col-md-12">Choose parameters</label>

            <div class="col-md-4">
                <select class="form-control">
                    <option> </option>
                    <option>Algorithm A result 1</option>
                    <option>Algorithm B result 2</option>
                </select>
            </div>
        <br/>
    </div>
    <div class="form-group">
            <label class="col-md-12">Parameter 1</label>
            <div class="col-md-4">
                <input type="text" class="form-control">
            </div>
            <label class="col-md-12">Parameter 2</label>
            <div class="col-md-4">
                <input type="text" class="form-control">
            </div>
    </div>
    <br/>
    <div class="form-group">
        <div class="col-md-2">
            <button class="btn btn-primary btn-block">Run</button>
        </div>
    </div>
    <hr/>
    <div class="form-group">
        <div class="col-md-12">
            <h4> Progress rate </h4>
        </div>
        <div class="col-md-12">
            <div class="progress">
                <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
                    60%
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <label> Now running Algorithm A</label>
        </div>
    </div>
    <hr/>

    <div class="form-group">
        <div class="col-md-12"><h4>Output data</h4></div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <table class="table table-bordered">
                <tr>
                    <th>id</th>
                    <th>Tag</th>
                    <th>Attribute1</th>
                    <th>Attribute2</th>
                </tr>
                <tr>
                    <td>1</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td>2</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <td>3</td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
            </table>
            <ul class="pagination">
                <li>
                <a href="#" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                </a>
                </li>
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li>
                <a href="#" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
                </li>
            </ul>
        </div>

    </div>
    <br/>

    <!-- 结束抄的运行 -->
    <div class="form-group">
        <label class="col-md-12">Output filename</label>
        <div class="col-md-4">
            <input type="text" class="form-control" placeholder="Algorithm A output i">
        </div>
        <div class="col-md-2">
            <button class="btn btn-default btn-block">Save in server</button>
        </div>
        <div class="col-md-2">
            <button class="btn btn-default btn-block">Download</button>
        </div>
    </div>
    <hr/>
    <div class="form-group">
        <div class="col-md-12">
            <h4>Save as use case</h4>
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-12">Use case name</label>
        <div class="col-md-4">
            <input type="text" class="form-control" placeholder="Algorithm A parameters i">
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-12">Use case description</label>
        <div class="col-md-8">
            <textarea class="form-control" row="5" placeholder="Algorithm A parameters i"></textarea>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-2">
            <button class="btn btn-primary btn-block">Save use case</button>
        </div>
    </div>


</form>

