        <!-- Main jumbotron for a primary marketing message or call to action -->
        <!-- 页面标题 -->
        <div class="container">
            <h1>新建数据分析流程模型</h1>
            <hr/>
        </div>

        <!-- 表单 -->
        <div class="container">

            <%= form_for :process_information do |f| %>
                <!--<form class="form-horizontal" role="form">-->
                <div class="form-group">
                    <label class="col-sm-2 control-label">数据分析流程名称</label>
                    <div class="col-sm-9">
                        <%= >
                        <input type="text" class="form-control" placeholder="数据分析流程名称">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">数据分析流程简介</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control" placeholder="数据分析流程简介">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">作者</label>
                    <div class="col-sm-4">
                        <input type="text" class="form-control" placeholder="作者">
                    </div>
                    <label class="col-sm-1 control-label">创建时间</label>
                    <div class="col-sm-4">
                        <input type="datetime" class="form-control" placeholder="2017-xx-xx">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">版本号</label>
                    <div class="col-sm-4">
                        <input type="text" class="form-control" placeholder="0.0.1">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label">分类</label>
                    <div class="col-sm-4">
                        <select class="form-control">
                            <option>类别1</option>
                            <option>类别2</option>
                            <option>类别3</option>
                            <option>类别4</option>
                            <option>类别5</option>
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-2 control-label">标签</label>
                    <div class="col-sm-3">
                        <input type="text" class="form-control" placeholder="标签名">
                    </div>
                    <div class="col-sm-1">
                        <button class="btn btn-default">新建</button>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <p>
                        <span class="label label-default">示例标签1</span>
                        <span class="label label-default">示例标签2</span>
                        <span class="label label-default">示例标签3</span>
                        </p>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label">核心算法</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control" placeholder="算法名称">
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label">功能描述</label>
                    <div class="col-sm-9">
                        <textarea class="form-control" rows="5">
                        </textarea>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label">输入格式</label>
                    <div class="col-sm-2">
                        <input type="text" class="form-control" placeholder="元素名">
                    </div>
                    <div class="col-sm-1">
                        <select class="form-control">
                            <option>int</option>
                            <option>double</option>
                            <option>string</option>
                            <option>time</option>
                        </select>
                    </div>
                    <div class="col-sm-1">
                        <button class="btn btn-default">添加</button>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-3">
                        <select class="form-control">
                            <option>分割符 空格</option>
                            <option>分割符 ,</option>
                            <option>分割符 ;</option>
                            <option>分割符 tab</option>
                        </select>
                    </div>
                    <div class="col-sm-1">
                        <button class="btn btn-default">添加</button>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <p>
                        <span class="label label-default">元素1 int</span>
                        <span class="label label-primary">分割符 空格</span>
                        <span class="label label-default">元素2 double</span>
                        </p>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label">输出格式</label>
                    <div class="col-sm-2">
                        <input type="text" class="form-control" placeholder="元素名">
                    </div>
                    <div class="col-sm-1">
                        <select class="form-control">
                            <option>int</option>
                            <option>double</option>
                            <option>string</option>
                            <option>time</option>
                        </select>
                    </div>
                    <div class="col-sm-1">
                        <button class="btn btn-default">添加</button>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-3">
                        <select class="form-control">
                            <option>分割符 空格</option>
                            <option>分割符 ,</option>
                            <option>分割符 ;</option>
                            <option>分割符 tab</option>
                        </select>
                    </div>
                    <div class="col-sm-1">
                        <button class="btn btn-default">添加</button>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <p>
                        <span class="label label-default">元素1 int</span>
                        <span class="label label-primary">分割符 空格</span>
                        <span class="label label-default">元素2 double</span>
                        </p>
                    </div>
                </div>
                <div class="form-group">
                    <!-- Button trigger modal -->
                    <label class="col-sm-2 control-label">案例</label>
                    <div class="col-sm-2">
                    <button type="button" class="btn btn-default" data-toggle="modal" data-target="#myModal">
                        新建案例
                    </button>
                    </div>
                </div>
                <div class="form-group">
                    <!-- Button trigger modal -->
                    <div class="col-sm-offset-10 col-sm-2">
                    <button type="submit" class="btn btn-primary">提交</button>
                    </div>
                </div>

                <!-- </form> -->
            <% end %>
        </div> <!-- /container -->
        <div class="container">
            <hr>
            <footer>
            <p>© SJTU 2017</p>
            </footer>
        </div> <!-- /container -->

        <!-- Modal -->
        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                        <h4 class="modal-title" id="myModalLabel">新建案例</h4>
                    </div>
                    <div class="modal-body">
                        <p>
                                <div class="form-group">
                                    <label>案例描述</label>
                                    <textarea type="text" class="form-control" placeholder="描述"></textarea>
                                </div>
                                <div class="form-group">
                                    <label for="caseInputFile">上传案例输入文件</label>
                                    <input type="file" id="caseInputFile">
                                </div>
                        </p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                        <button type="button" class="btn btn-primary">提交</button>
                    </div>
                </div>
            </div>
        </div>

        <!-- Bootstrap core JavaScript
        ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="jquery.min.js"></script>
        <script src="bootstrap.min.js"></script>
        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <script src="ie10-viewport-bug-workaround.js"></script>
