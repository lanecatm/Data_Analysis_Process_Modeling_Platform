<table class="table table-bordered">
    <tr>
        <th rowspan="10">Basic information</th>
        <th> Model name</th>
        <td><%= @process_information.name %></td>
    </tr>
    <tr>
        <th>Model introduction</th>
        <td><%= @process_information.introduction %></td>
    </tr>
    <tr>
        <th>Author</th>
        <td><%= @process_information.author %></td>
    </tr>

    <tr>
        <th>Create time</th>
        <td><%= @process_information.createtime %></td>
    </tr>

    <tr>
        <th>Version</th>
        <td><%= @process_information.version %></td>
    </tr>

    <tr>
        <th>Last version</th>
        <td> </td>
    </tr>
    <tr>
        <th>Category</th>
        <td><%= @category_name %></td>
    </tr>

    <tr>
        <th>Tag</th>
        <td>
            <span class="label label-default">Tag 1</span>
            <span class="label label-default">Tag 2</span>
            <span class="label label-default">Tag 3</span>
        </td>
    </tr>

    <tr>
        <th>Algorithm</th>
        <!--
        <td><%= @process_information.algorithm %></td>
        -->
        <td></td>
    </tr>


    <tr>
        <th>Functional description</th>
        <td><%= @process_information.functional_description %></td>
    </tr>

    <tr>
        <th rowspan="3">Data format</th>
        <th>Separator</th>
        <td>
            Space
        </td>
    </tr>
    <tr>
        <th>Input format</th>
        <td>
            <span class="label label-default">Column1 int</span>
            <span class="label label-default">Column2 double</span>
        </td>
    </tr>


    <tr>
        <th>Output format</th>
        <td>
            <span class="label label-default">Column1 int</span>
            <span class="label label-default">Column2 double</span>
        </td>
    </tr>
    <tr>
        <th rowspan="6">Case download</th>
        <td rowspan="2">Case1</td>
        <td>
            Case1 description
        </td>
    </tr>

    <tr>
        <td>
            <button type="button" class="btn btn-primary btn-right">Download</button>
        </td>
    </tr>

    <tr>
        <td rowspan="2">Case2</td>
        <td>
            Case2 description
        </td>
    </tr>

    <tr>
        <td>
            <button type="button" class="btn btn-primary btn-right">Download</button>
        </td>
    </tr>

    <tr>
        <td rowspan="2">Case3</td>
        <td>
            Case3 description
        </td>
    </tr>

    <tr>
        <td>
            <button type="button" class="btn btn-primary btn-right">Download</button>
        </td>
    </tr>


</table>

