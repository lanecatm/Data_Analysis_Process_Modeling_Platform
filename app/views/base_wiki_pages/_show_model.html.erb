<div class="col-md-12">
    <table class="table table-striped table-bordered">
        <tr>
            <th>Model name </th>
            <th>Version</th>
        </tr>
        <% @page.wiki_and_workflow_informations.each do |wiki_and_workflow_information|%>
            <% if wiki_and_workflow_information.workflow_information.status == WorkflowInformationsController::WORKFLOW_INFO_PUBLISH %>
                <tr>
                    <td>
                        <% if wiki_and_workflow_information.workflow_information.name == nil or wiki_and_workflow_information.workflow_information.name == ""  %>
                            <%= link_to "No Name", workflow_information_path(wiki_and_workflow_information.workflow_information) %>
                        <% else %>
                            <%= link_to wiki_and_workflow_information.workflow_information.name, workflow_information_path(wiki_and_workflow_information.workflow_information) %>
                        <% end %>
                    </td>
                    <td>
                        <%= wiki_and_workflow_information.workflow_information.version_name %>
                    </td>
                </tr>
            <% end %>
        <% end %>
    </table>

    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
</div>
