<div class="container">
    <div class="row">
        <br/>
        <br/>
    </div>
    <div class="row">

        <%= form_for :upload, remote: true, :html => {:class => "form-horizontal", :role => "form"}, url: data_pre_processing_index_path do |f|%>
            <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
            <div class="form-group">
                <label class="col-md-12">input data from local file</label>
                <div class="col-md-12">
                    <%= f.file_field :file %>
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-3">
                    <%= f.submit :"Upload", :class => "btn btn-primary btn-block" %>
                </div>
            </div>
        <% end %>
        <ul id="upload_files">
            <%= render '/data_pre_processing/upload_file' %>
        </ul>
    </div>
</div>
