<%= form_for :upload, :html => {:class => "form-horizontal", :role => "form"}, url: preprocess_upload_path do |f|%>
    <div class="form-group">
        <label class="col-md-12">input data from local file</label>
        <div class="col-md-12">
            <%= f.file_field :file %>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-3">
            <%= f.submit :"Upload", :class => "btn btn-primary btn-block" %>
        </div>
    </div>
<% end %>


