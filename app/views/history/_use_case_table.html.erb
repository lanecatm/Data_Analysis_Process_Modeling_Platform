
<table class="table table-striped table-bordered">
    <tr>
        <th>ID </th>
        <th>Use case name</th>
        <th>Update time</th>
    </tr>
    <% @process_informations.each do |process_information| %>
        <tr>
            <td>
                <%= process_information.id %>
            </td>
            <td>
                <%= link_to process_information.name, process_execute_path %>
            </td>
            <td>
                <%= process_information.updated_at %>
            </td>
        </tr>
    <% end %>
</table>
