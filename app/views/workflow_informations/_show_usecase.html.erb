
<table class="table table-striped table-bordered">
    <tr>
        <th>ID </th>
        <th>Name</th>
        <th>Author</th>
        <th>Update time</th>
    </tr>
    <% @process_informations.each do |process_information| %>
        <tr>
            <td>
                <%= process_information.id %>
            </td>
            <td>
                <%= process_information.name%>
            </td>
            <td>
                <p>Admin</p>
            </td>
            <td>
                <%= process_information.updated_at %>
            </td>
        </tr>
    <% end %>
</table>
