
<div class="col-md-12">
<div style="width:1100px; height:600px">
  <div class="content" id="js-drop-zone">

   <div class="message intro" >
      <div class="note">
        <a id="js-create-diagram" href></a>
      </div>
    </div>

    <div class="message error">
      <div class="note">
        <p>Ooops, we could not display the BPMN 2.0 diagram.</p>

        <div class="details">
          <span>cause of the problem</span>
          <pre></pre>
        </div>
      </div>
    </div>

    <div class="canvas" id="js-canvas" style="width:100%; height:100%"></div>
    <div id="js-properties-panel"></div>
  </div>

    <%= form_for :article, url: pannel_new_path, :html => {:id => 'data_form'} do |f| %>
      <%= f.hidden_field :title %>
      <%= f.hidden_field :workflow_information_id, :value => params[:id] %>
      <%= f.hidden_field :xml%>
      <%= f.hidden_field :selected_element%>
      <%= f.submit :try %>
    <% end %>
<%= params[:debug_info] %>

